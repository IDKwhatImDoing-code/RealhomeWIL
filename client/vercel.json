{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" }
      }
    ],
    
      "buildCommand": "npm run build",
      "outputDirectory": "client/dist",
      "installCommand": "npm install",
    
    "routes": [
      { "src": "/(.*)", "dest": "/index.html" }
    ]
  }